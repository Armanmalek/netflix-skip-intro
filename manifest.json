{
  "name": "Neflix Auto Skip Intro Button",
  "description": "Automatically presses skip button on Netflix on any episode",
  "version": "0.0.0.4",
  "manifest_version": 2,
    "browser_action": {
        "default_icon": "Netflix-new-icon.png"
    },
  "content_scripts": [
    {
      "matches" : ["https://www.netflix.com/*"],
      "js"      : ["jquery.min.js", "skip.js"]
    }
  ]
}
